order += "ethernet eth0"
order += "ipv6"
order += "load"
order += "tztime mtv"
order += "tztime utc"
order += "tztime local"

ethernet eth0 {
  format_up = "%ip"
  format_down = "Ethernet down"
}

load {
  format = "%5min"
}

tztime mtv {
  format = "%Z %H:%M:%S"
  timezone = "America/Los_Angeles"
}

tztime utc {
  format = "%Z %H:%M:%S"
  timezone = "UTC"
}

tztime local {
  format = "%Y-%m-%d %H:%M:%S"
}
